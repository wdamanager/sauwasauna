# WordPress GraphQL Endpoint
WORDPRESS_GRAPHQL_URL=http://localhost/graphql

# Google Tag Manager Configuration
PUBLIC_GTM_ID=YOUR_GTM_CONTAINER_ID

# Google Analytics 4 Configuration (managed through GTM)
# Keep for reference - GA4 will be configured inside GTM
PUBLIC_GA4_MEASUREMENT_ID=YOUR_GA4_MEASUREMENT_ID
PUBLIC_GA4_STREAM_ID=YOUR_GA4_STREAM_ID

# Environment (development, staging, production)
PUBLIC_ENV=development